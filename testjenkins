node{
   stage('SCM Checkout'){
     git 'https://github.com/kadirsahan/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'maven-3', type: 'maven'   
      sh "${mvnHome}/bin/mvn package"
   }
   stage('just-test'){
     echo "${BUILD_NUMBER}"
   }
   stage('just-test'){
     def MYBUILD_NUMBER = getBinding().getVariables()['BUILD_NUMBER']
     print $MYBUILD_NUMBER
   }
}
